<UserControl x:Class="Gold_Client.View.TabWindows.SuggestControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" Height="250"
             Width="420">

    <Grid>
        <Label Content="Topic:" Height="25"  Margin="16,10,363,215" Name="label1"  Width="41" />
        <TextBox Height="23"  Margin="62,10,24,217" Text="{Binding TopicSuggestMessageTB, UpdateSourceTrigger=PropertyChanged}" />
        <Label Content="Massage:" Height="27"  Margin="10,33,316,190" Name="label2"  Width="94" />
        <TextBox Height="145"  Margin="10,60,24,45" Text="{Binding SuggestMessageTB, UpdateSourceTrigger=PropertyChanged}"  Width="385" />
        <Button Content="Send" Height="27"  Margin="164,214,183,10" Command="{Binding SendSuggestCommand}" Width="70"  />
    </Grid>
</UserControl>
