<Window x:Class="Gold.serverAsk"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gold"
        mc:Ignorable="d"
        Title="serverAsk" Height="140" Width="290">
    <Grid>
        <TextBox x:Name="answerTB" Height="23" Margin="10,43" PreviewKeyDown="EnterClicked" Width="262"/>
        <Button x:Name="button" Content="Send" Margin="202,71,10,10" Click="button_Click" />
        <Label Content="Label" Margin="10,10,10,71" Background="#FF060606" Foreground="Yellow" Width="262"/>
    </Grid>
</Window>
